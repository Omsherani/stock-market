{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "NIXPACKS"
    },
    "deploy": {
        "startCommand": "export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/nix/var/nix/profiles/default/lib && /opt/venv/bin/gunicorn --bind 0.0.0.0:$PORT --pythonpath backend app:app"
    }
}